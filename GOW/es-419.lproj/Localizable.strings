/* WEAPONS */
"weapon.name.boomshot" = "Lanzagranadas Boomshot";
"weapon.description.boomshot" = "Era un lanzagranadas de acción de bomba de un solo disparo, diseñado y utilizado principalmente por los Locust, en particular por el Boomer.";
"weapon.name.hammerburstii" = "Hammerburst II";
"weapon.description.hammerburstii" = "Era el fusil de asalto estándar de alta potencia de los Locust. A menudo se le denomina la contraparte Locust del fusil de asalto COG Lancer.";
"weapon.name.mulcher" = "Mulcher";
"weapon.description.mulcher" = "El Mulcher es un pesado fusil Gatling manual giratorio desarrollado por la Coalición de Gobiernos Ordenados.";
"weapon.name.bolo_grenade" = "Bologranada";
"weapon.description.bolo_grenade" = "La Bologranada, también conocida como Granada de Fragmentación, era la granada de fragmentación estándar utilizada por la Coalición de Gobiernos Ordenados, la Horda Locust y el Enjambre.";
"weapon.name.boltok" = "Pistola Boltok";
"weapon.description.boltok" = "Era el arma estándar de los Locust, especialmente de los Drones. La pistola continúa la filosofía de diseño de armas de los Locust, que enfatiza la fuerza bruta.";
"weapon.name.lancer" = "Fusil de Asalto Lancer Mark 1";
"weapon.description.lancer" = "Apodado Retro Lancer durante la Pandemia Lambent, fue el predecesor del fusil de asalto Lancer Mark 2. Anteriormente, el fusil de asalto principal del ejército de la Coalición de Gobiernos Ordenados se utilizó durante la Guerra del Péndulo.";
"weapon.name.lancer2" = "Fusil de Asalto Lancer Mark 2";
"weapon.description.lancer2" = "Una actualización del fusil de asalto Lancer Mark 1, fabricado con una bayoneta motosierrada, mayor precisión, menor retroceso y mayor capacidad de munición, pero con un poder de fuego inferior en comparación con su predecesor.";
"weapon.name.sniper" = "Fusil Francotirador Longshot";
"weapon.description.sniper" = "Era un fusil francotirador de un solo disparo y de gran calibre. Creado por la Coalición de Gobiernos Ordenados durante las Guerras del Péndulo, el Longshot se utilizó incluso diecisiete años después del Día de la Emergencia.";
"weapon.name.hammer_of_dawn" = "El Martillo del Alba";
"weapon.description.hammer_of_dawn" = "El Martillo del Alba era un láser orbital, basado en satélite, alimentado por Imulsión COG. Sus componentes clave fueron inventados por el científico de la UIR, Mauris Ivo, y completados por el profesor Adam Fenix después de haber sido robados durante la Operación: Nivelador.";
"weapon.name.mx8" = "Pistola Snub MX8";
"weapon.description.mx8" = "Era la arma secundaria estándar para el personal de las fuerzas armadas de la Coalición de Gobiernos Ordenados.";




